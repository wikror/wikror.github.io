source "https://rubygems.org"

# Jekyll version
gem 'jekyll', '>=3.0.0'

# Plugins
group :jekyll_plugins do
  gem "jekyll-feed", "~> 0.6"
  gem 'jekyll-paginate-v2'
  gem 'jekyll-sitemap'
  gem 'jekyll-seo-tag'
  gem 'tzinfo-data', platforms: [:mingw, :mswin, :x64_mingw, :jruby]
end

# Web server for Jekyll 3+
gem "webrick", "~> 1.8"
